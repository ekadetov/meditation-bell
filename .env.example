# Awakening Bell - Environment Variables

# Application Information
VITE_APP_NAME=Awakening Bell
VITE_APP_VERSION=1.0.0

# Storage Configuration
# Maximum localStorage quota in MB (browser default is typically 5-10MB)
VITE_STORAGE_QUOTA_MB=50

# Session Retention
# How long to keep session data (in days)
VITE_SESSION_RETENTION_DAYS=90

# Feature Flags (optional - defaults to true)
VITE_ENABLE_MOOD_TRACKING=true
VITE_ENABLE_INSIGHTS=true
VITE_ENABLE_ANALYTICS=false

# Development Only
# NODE_ENV is set automatically by Vite
# development, production, or test
